{
    "default": {
        "dryRun": false,
        "formatOptions": {
            "snipperInterface": "async-await"
        },
        "paths": [
            "e2etests/tests/features/"
        ],
        "require": [
            "e2etests/tests/steps/**/*.ts",
            "e2etests/corelib/basepage.spec.ts"
        ],
        "requireModule": [
            "ts-node/register"
        ],
        "format": [
            [
                "json",
                "reports/cucumber_report.json"
            ],
            [
                "junit",
                "reports/junit.xml"
            ],
            [
                "html",
                "reports/html-formatter.html"
            ]
        ]
    }
}